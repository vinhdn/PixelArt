<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_pressed="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <corners n1:radius="@dimen/dimen_3dp" />
            <solid n1:color="#60999999" />
        </shape>
    </item>
    <item n2:state_pressed="false"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <corners n2:radius="@dimen/dimen_3dp" />
            <solid n2:color="@color/null_color" />
        </shape>
    </item>
</selector>